        "husky": "^8.0.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "prettier": "^2.8.6",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2",
        "vite": "^4.2.1",
        "vite-plugin-webpackchunkname": "^0.2.4"
