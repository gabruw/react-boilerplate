{
    "private": true,
    "name": "react-boilerplate",
    "description": "A react boilerplate",
    "version": "0.0.1",
    "type": "module",
    "license": "MIT",
    "author": "gabruw",
    "keywords": [],
    "repository": {
        "type": "git",
        "url": "https://github.com/gabruw/react-boilerplate"
    },
    "scripts": {
        "dev": "vite",
        "test": "jest",
        "preview": "vite preview",
        "build": "tsc && vite build",
        "format": "prettier --write .",
        "check:format": "prettier --check src",
        "check:types": "tsc --pretty --noEmit",
        "check:lint": "eslint src --ext .ts,.tsx",
        "prepare": "husky install",
        "mount": "npm install -g npm-check-updates"
    },
    "dependencies": {
        "@apollo/client": "^3.7.10",
        "@chakra-ui/react": "^2.5.2",
        "@chakra-ui/theme": "^2.2.4",
        "@emotion/react": "^11.10.6",
        "@emotion/styled": "^11.10.6",
        "@hookform/resolvers": "^3.0.0",
        "@reduxjs/toolkit": "^1.9.3",
        "framer-motion": "^10.6.1",
        "graphql": "^16.6.0",
        "i18next": "^22.4.13",
        "i18next-browser-languagedetector": "^7.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.7",
        "react-i18next": "^12.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.9.0",
        "react-secure-storage": "^1.1.0",
